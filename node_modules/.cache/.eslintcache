t-icons/io\";\n\n\nfunction App() {\n\n\n  const [coins, setCoins] = useState([])\n  const [search, setSearch] = useState('')\n  const [cur, setcur] = useState(\"usd\")\n  const [book, setbook] = useState(false)\n\n  const changeHandler = e => {\n    setSearch(e.target.value)\n  }\n\n  const coinFilter = coins.filter(coin =>\n    coin.name.toLowerCase().includes(search.toLowerCase())\n  )\n\n\n  function bookmode() {\n    if (book) {\n      setbook(false);\n    }\n    else {\n      setbook(true);\n    }\n  }\n\n\n\n  useEffect(() => {\n    axios.get(`https://api.coingecko.com/api/v3/coins/markets?vs_currency=${cur}&order=market_cap_desc&per_page=100&page=1&sparkline=false`)\n      .then(res => {\n        setCoins(res.data);\n      }).catch(err => {\n        console.log(err);\n        alert('Error');\n      });\n  }, []);\n\n\n  return (\n    <div className=\"container\">\n      <div className=\"nav\">\n        <NavBar />\n        <div className=\"search\">\n          <div className=\"curSwitch\">\n            <select\n              className=\"curChange\"\n              onChange={(e) => {\n                const selected = e.target.value;\n                setcur(selected);\n                axios.get(`https://api.coingecko.com/api/v3/coins/markets?vs_currency=${cur}&order=market_cap_desc&per_page=100&page=1&sparkline=false`)\n                  .then(res => {\n                    setCoins(res.data);\n                  })\n\n              }}\n            >\n              <option value=\"usd\">USD</option>\n              <option value=\"czk\">CZK</option>\n            </select>\n          </div>\n          <div className=\"Bookmarks\" onClick={bookmode}>\n\n            {(() => {\n              if (book === true) {\n                return (\n                  <IoMdHeart size={50} color={\"red\"} />\n                )\n              } else {\n                return (\n                  <IoMdHeartEmpty size={50} color={\"red\"} />\n                )\n              }\n            })()}\n          </div>\n          <form>\n            <input type=\"text\" placeholder=\"Search\" className=\"search-input\" onChange={changeHandler} />\n          </form>\n        </div>\n      </div>\n\n\n      <div className=\"invest\">\n        <div className=\"invData\">\n\n        </div>\n      </div>\n\n      <Placeholder />\n      {\n        coinFilter.map(coin => {\n          return (\n            <div>\n              <Coin\n                key={coin.id}\n                coinName={coin.name}\n                icon={coin.image}\n                symbol={coin.symbol}\n                volume={coin.total_volume}\n                price={coin.current_price}\n                priceChange24={coin.price_change_percentage_24h}\n                marketcap={coin.market_cap}\n                cur={cur}\n                id={coin.id}\n              />\n            </div>\n          );\n        })}\n\n    </div>\n  );\n}\n\n\nexport default App;\n","C:\\Users\\Michal\\Desktop\\TW_Project\\crypto-calculator-v1\\src\\components\\Coin.js",[],"C:\\Users\\Michal\\Desktop\\TW_Project\\crypto-calculator-v1\\src\\components\\NavBar.js",["28"],"import React from 'react'\r\nimport '../App.css';\r\nimport logo from '../icons/logo.png';\r\n\r\nconst NavBar = () => {\r\n    return (\r\n        <div className=\"navbar\">\r\n            <div className=\"leftside\">\r\n            <a class=\"logo\" href=\"#\"><img src={logo} alt=\"logo\"/></a>\r\n            <h1 className=\"site-name\">Crypto Calculator</h1>\r\n            </div>\r\n\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default NavBar\r\n","C:\\Users\\Michal\\Desktop\\TW_Project\\crypto-calculator-v1\\src\\components\\Placeholder.js",[],{"ruleId":"29","replacedBy":"30"},{"ruleId":"31","replacedBy":"32"},{"ruleId":"33","severity":1,"message":"34","line":47,"column":6,"nodeType":"35","endLine":47,"endColumn":8,"suggestions":"36"},{"ruleId":"37","severity":1,"message":"38","line":9,"column":13,"nodeType":"39","endLine":9,"endColum